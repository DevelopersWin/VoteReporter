<?xml version="1.0" encoding="utf-8" standalone="no"?>
<Project ToolsVersion="14.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
	<PostSharpTargetVersion>4.3.23</PostSharpTargetVersion>
	  <PostSharpDisabledMessages>PS0003</PostSharpDisabledMessages>
	<NuGetPackageRoot Condition="'$(NuGetPackageRoot)' == ''">$(UserProfile)\.nuget\packages\</NuGetPackageRoot>	
	<PostSharpSearchPath>$(NuGetPackageRoot)System.Net.Http\4.1.0\lib\net46</PostSharpSearchPath>
	<PostSharpHostConfigurationFile Condition="Exists( 'App.config' )">App.config</PostSharpHostConfigurationFile>
  </PropertyGroup>
  <!--<Target Name="Default">
	<Message Text="$(TargetName): $(AppConfig)" Importance="High" />
  </Target>-->
  <ImportGroup>
	<Import Project="$(NuGetPackageRoot)PostSharp\$(PostSharpTargetVersion)\tools\PostSharp.targets" Condition="Exists('$(NuGetPackageRoot)PostSharp\$(PostSharpTargetVersion)\tools\PostSharp.targets')" />
  </ImportGroup>  
</Project>