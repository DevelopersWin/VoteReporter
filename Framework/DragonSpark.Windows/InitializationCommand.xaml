<configuration:InitializationCommandBase x:Class="DragonSpark.Windows.InitializationCommand"
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:windows="clr-namespace:DragonSpark.Windows"
	xmlns:typeSystem="clr-namespace:DragonSpark.TypeSystem;assembly=DragonSpark"
	xmlns:configuration="clr-namespace:DragonSpark.Configuration;assembly=DragonSpark"
	mc:Ignorable="d">
	<windows:ApplyAttributeProviderConfiguration Parameter="{x:Static typeSystem:AttributeProviderHost.Instance}" Factory="{x:Static windows:AttributeProviderLocator.Instance}" />
</configuration:InitializationCommandBase>