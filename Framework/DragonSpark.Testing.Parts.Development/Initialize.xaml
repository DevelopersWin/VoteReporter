<configuration:InitializeFrameworkConfigurationCommand x:Class="DragonSpark.Testing.Parts.Development.Initialize"
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:configuration="clr-namespace:DragonSpark.Configuration;assembly=DragonSpark"
	xmlns:ds="http://framework.dragonspark.us"
	xmlns:diagnostics="clr-namespace:DragonSpark.Windows.Diagnostics;assembly=DragonSpark.Windows"
	xmlns:categories="clr-namespace:DragonSpark.Diagnostics.Logger.Categories;assembly=DragonSpark"
	mc:Ignorable="d">
	<configuration:FrameworkConfiguration>
		<configuration:FrameworkConfiguration.Diagnostics>
			<configuration:Diagnostics Level="Debug" ProfilerFactoryType="{ds:GenericType diagnostics:ProfilerFactory(categories:Debug)}" />
		</configuration:FrameworkConfiguration.Diagnostics>
	</configuration:FrameworkConfiguration>
</configuration:InitializeFrameworkConfigurationCommand>